<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.NotificationsMapper">

	<!-- お知らせ情報　登録 -->
	<insert id="insert"
		parameterType="com.example.demo.entity.Notifications"
		useGeneratedKeys="true" keyProperty="id">
		INSERT INTO notifications (content, notification_type, created_at)
		VALUES (#{content}, #{notificationType}, #{createdAt})
	</insert>


</mapper>